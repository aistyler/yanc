{
  "name": "@yanc/env-babel",
  "version": "1.0.0",
  "main": "_yanc/index.js",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register ./_src/index.ts",
    "dev:lib": "jest --watch ./_src/*",
    "start": "node dist/index.js",
    "dist": "yarn dist:clean && babel ./_src -x .js,.jsx,.ts,.tsx --out-dir ./dist --env-name production && ls -al ./dist",
    "dist:clean": "rm -rf ./dist",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "test": "jest ./_src",
    "all": "yarn test && yarn dev && yarn lint && yarn dist && yarn start && yarn dist:clean"
  },
  "files": [
    ".babelrc.js",
    ".eslintrc.js",
    "jest.config.js",
    "pathconfig.json"
  ],
  "dependencies": {
    "@babel/cli": "~7.14.8",
    "@babel/core": "~7.15.0",
    "@babel/eslint-parser": "~7.15.0",
    "@babel/plugin-proposal-class-properties": "~7.14.5",
    "@babel/plugin-proposal-decorators": "~7.14.5",
    "@babel/plugin-transform-runtime": "~7.15.0",
    "@babel/plugin-transform-typescript": "~7.15.0",
    "@babel/preset-env": "~7.15.0",
    "@babel/preset-typescript": "~7.15.0",
    "@types/jest": "~27.0.1",
    "@types/node": "~16.7.0",
    "@typescript-eslint/eslint-plugin": "~4.30.0",
    "@typescript-eslint/parser": "~4.30.0",
    "babel-jest": "~27.1.0",
    "babel-plugin-module-resolver": "~4.1.0",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-import-resolver-babel-module": "~5.3.1",
    "eslint-plugin-import": "~2.24.2",
    "eslint-plugin-jest": "~24.4.0",
    "eslint-plugin-json": "~3.1.0",
    "eslint-plugin-prettier": "~4.0.0",
    "jest": "~27.1.0",
    "lodash": "^4.17.21",
    "prettier": "~2.3.2",
    "ts-node": "~10.2.1",
    "tsconfig-paths": "~3.11.0",
    "typescript": "~4.4.2"
  },
  "peerDependencies": {
    "@babel/cli": "~7.14.8",
    "eslint": "~7.32.0",
    "jest": "~27.1.0"
  }
}
