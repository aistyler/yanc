{
  "name": "@yanc/cli",
  "version": "1.1.0",
  "bin": {
    "yanc": "bin/yanc.js"
  },
  "dependencies": {
    "yargs": "~17.1.1"
  },
  "scripts": {
    "dev": "node ./_yanc/run.js",
    "start": "yanc",
    "lint": "yanc babel eslint .",
    "lint:fix": "yarn lint --fix",
    "test": "yanc babel jest .",
    "dist": "yanc babel babel ./_yanc --out-dir ./dist --env-name production",
    "dist:clean": "rm -rf ./dist",
    "yanc:export": "yanc babel export",
    "ver": "yarn version -i"
  },
  "devDependencies": {
    "@babel/cli": "~7.14.8",
    "@yanc/env-babel": "*",
    "eslint": "~7.32.0",
    "jest": "~27.1.0"
  },
  "yanc": {
    "verbose": true
  }
}
