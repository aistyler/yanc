{
  "name": "@yanc/cli",
  "private": true,
  "version": "1.0.0",
  "bin": {
    "yanc": "bin/yanc.js"
  },
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/run.js",
    "dev:lib": "jest --watch src/*",
    "start": "node dist/index.js",
    "dist": "yarn dist:clean && babel --out-dir ./dist ./src -x .js,.jsx,.ts,.tsx --env-name production && ls -al ./dist",
    "dist:clean": "rm -rf ./dist",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "test": "jest src",
    "all": "yarn test && yarn lint && yarn dist && yarn dev"
  },
  "dependencies": {
    "yargs": "~17.1.1"
  },
  "devDependencies": {
    "@babel/cli": "~7.14.5",
    "@babel/core": "~7.14.0",
    "@babel/eslint-parser": "~7.14.0",
    "@babel/plugin-proposal-decorators": "~7.14.0",
    "@babel/plugin-transform-runtime": "~7.14.0",
    "@babel/plugin-transform-typescript": "~7.14.0",
    "@babel/preset-env": "~7.14.0",
    "@babel/preset-typescript": "~7.14.0",
    "@types/jest": "~26.0.0",
    "@types/node": "~16.3.0",
    "@typescript-eslint/eslint-plugin": "~4.28.0",
    "@typescript-eslint/parser": "~4.28.0",
    "babel-plugin-module-resolver": "~4.1.0",
    "eslint": "~7.26.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-import-resolver-babel-module": "~5.3.1",
    "eslint-plugin-import": "~2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-json": "~3.0.0",
    "eslint-plugin-prettier": "~3.4.0",
    "jest": "~27.0.6",
    "prettier": "~2.3.0",
    "ts-jest": "~27.0.3",
    "ts-node": "~10.1.0",
    "tsconfig-paths": "~3.10.1",
    "typescript": "~4.3.5"
  }
}
