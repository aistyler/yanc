{
  "name": "@yanc/cli",
  "version": "1.3.1",
  "bin": {
    "yanc": "bin/yanc.js"
  },
  "dependencies": {
    "yargs": "~17.1.1"
  },
  "scripts": {
    "dev": "yanc babel exec ./src/cli.js",
    "test": "yanc babel test .",
    "lint": "yanc babel lint .",
    "lint:fix": "yarn lint --fix",
    "dist": "yanc babel build ./src --out-dir ./dist --env-name production",
    "dist:clean": "rm -rf ./dist",
    "start": "node ./bin/yanc.js",
    "conf": "yanc babel configure",
    "conf:reset": "yanc babel configure --reset",
    "ver": "yarn version -i"
  },
  "devDependencies": {
    "@yanc/env-babel": "*"
  },
  "yanc": {
    "verbose": true
  }
}
