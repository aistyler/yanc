{
  "name": "@yanc/cli",
  "version": "1.0.2",
  "bin": {
    "yanc": "bin/yanc.js"
  },
  "dependencies": {
    "yargs": "~17.1.1"
  },
  "scripts": {
    "dev": "node ./_yanc/run.js",
    "start": "yanc",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "dist": "babel ./_yanc --out-dir ./dist --env-name production",
    "dist:clean": "rm -rf ./dist",
    "gen": "yanc babel update",
    "ver": "yarn version -i"
  },
  "devDependencies": {
    "@babel/cli": "~7.14.8",
    "@yanc/env-babel": "*",
    "eslint": "~7.32.0",
    "jest": "~27.1.0"
  },
  "yanc": {
    "verbose": true
  }
}
